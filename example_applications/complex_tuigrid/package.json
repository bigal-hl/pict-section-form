{
    "name": "complex_tuigrid_application",
    "version": "1.0.0",
    "description": "The most complex TuiGrid application ever!",
    "main": "Complex-TuiGrid-Application.js",
    "scripts": {
        "start": "node Complex-TuiGrid-Application.js",
        "build": "npx quack build && npx quack copy"
    },
    "author": "steven",
    "license": "MIT",
    "devDependencies": {
        "tui-grid": "^4.21.22"
    },
    "copyFilesSettings": {
        "whenFileExists": "overwrite"
    },
    "copyFiles": [
        {
            "from": "./html/*",
            "to": "./dist/"
        },
        {
            "from": "./node_modules/tui-grid/dist/*",
            "to": "./dist/"
        },
        {
            "from": "./node_modules/tui-date-picker/dist/*",
            "to": "./dist/"
        },
        {
            "from": "../../node_modules/pict/dist/*",
            "to": "./dist/"
        }
    ]
}
